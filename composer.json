{
  "name": "phrlog/zvonok-client",
  "description": "Client for Zvonok(calltools) API",
  "type": "library",
  "keywords": [
    "zvonok",
    "calltools",
    "client"
  ],
  "homepage": "https://github.com/phrlog/zvonok-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Khatin Konstantin",
      "email": "phrlog@gmail.com",
      "role": "Developer"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "netresearch/jsonmapper": "^1.4",
    "nyholm/psr7": "^1.2",
    "php-http/curl-client": "^1.7",
    "php-http/discovery": "^1.7",
    "php-http/httplug": "^1.1",
    "php-http/message": "^1.7",
    "php-http/message-factory": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Phrlog\\Zvonok\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phrlog\\Zvonok\\Tests\\": "tests/"
    }
  }
}
